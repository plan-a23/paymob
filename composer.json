{
    "name": "plan-a23/paymob",
    "description": ":package_description",
    "license": "MIT",
    "authors": [
        {
            "name": "Plan A",
            "email": "contact@plan-a.eu.org",
            "homepage": "https://plan-a.eu.org"
        }
    ],
    "homepage": "https://github.com/plan-a23/paymob",
    "keywords": ["Laravel", "Paymob"],
    "require": {
